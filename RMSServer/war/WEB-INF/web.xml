<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
    <servlet>
        <servlet-name>settings</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.SettingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>settings</servlet-name>
        <url-pattern>/settings</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>status</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.StatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>status</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>manual_status</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.ManualStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>manual_status</servlet-name>
        <url-pattern>/manual_status</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>alert</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.AlertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>alert</servlet-name>
        <url-pattern>/alert</url-pattern>
    </servlet-mapping>
        
    
    <servlet>
        <servlet-name>data</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.AccelerometerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>data</servlet-name>
        <url-pattern>/data</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>start</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.AlgorithmsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>start</servlet-name>
        <url-pattern>/_ah/start</url-pattern>
    </servlet-mapping> 
   
    
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>biometrics</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.BiometricsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>biometrics</servlet-name>
        <url-pattern>/biometrics</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>datastore</servlet-name>
        <servlet-class>com.gmail.utexas.rmsystem.DatastoreServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>datastore</servlet-name>
        <url-pattern>/datastore</url-pattern>
    </servlet-mapping>
    
    
    <welcome-file-list>
        <welcome-file>RMSServer.html</welcome-file>
    </welcome-file-list>
    
    
  

</web-app>
